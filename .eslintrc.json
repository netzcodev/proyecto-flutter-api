{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "plugins": ["import"],
  "rules": {
    "no-console": "warn",
    "import/no-cycle": ["warn", { "maxDepth": 2 }]
  }
}
